<h1>
    <%= @room.room_name %>
</h1>

<div>
    <% @chats.each do |chat| %>
        <%= render 'rooms/chat', chat: chat %>
    <% end %>
</div>

<div>
    <%= render 'form', chat_text: @chat_text, path_to_post: record_chat_room_path %>
    <button type="button" class="send_content">送信</button>
</div>